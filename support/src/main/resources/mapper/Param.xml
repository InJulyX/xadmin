<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xadmin.support.core.mapper.ParamMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xadmin.support.core.entity.Param">
        <id column="param_id" property="paramId" />
        <result column="param_name" property="paramName" />
        <result column="param_key" property="paramKey" />
        <result column="param_value" property="paramValue" />
        <result column="param_desc" property="paramDesc" />
        <result column="system_flag" property="systemFlag" />
        <result column="ct" property="ct" />
        <result column="ut" property="ut" />
        <result column="is_del" property="isDel" />
        <result column="version" property="version" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        param_id, param_name, param_key, param_value, param_desc, system_flag, ct, ut, is_del, version
    </sql>

</mapper>
